
<% 
    imageHelper = (folder, imageName, className = 'img_List') => {  
        let xhtml = '';
        if(imageName == null){
            imageName = 'no-avatar.png'
        }
        xhtml = `<img id="imgUpload" class="${className}" src="${imageName}" alt="${imageName}"/>`
        return xhtml;
    }    
%>


